{
  "compilerOptions": {
    "target": "es5",
    "incremental": false,
     "typeRoots": ["./node_modules/@types"],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "shared-types": ["../shared-types/src"]
    },
    "types": []
    // "types": ["cookie-parser"]
  },
  "include": ["src", "../shared-types/src", "../shared-types/core.ts", "../shared-types/customer.ts", "../shared-types/google.ts", "../shared-types/language.ts", "../shared-types/mapCoordinates.ts", "../shared-types/occupancy.ts", "../shared-types/report.ts", "../shared-types/spaceAssignment.ts", "../shared-types/workspaceMap.ts"]
}