{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    // "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "@types/*": ["../../types/*"]
    }
  },
  "include": ["src/**/*", "../../node_modules/types/**/*", "src/models/workspaceRoom/core.ts", "../../types/maplayout.ts", "../../types/MapFilters.ts", "../../types/mapcoordinates.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
}